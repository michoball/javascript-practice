{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": "off",
    "quotes": "off",
    "comma-dangle": "off",
    "space-before-function-paren": "off",
    "no-var": "off",
    "spaced-comment": "off"
  }
}
