{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "quotes": "off",
    "semi": "error",
    "no-undef": "off",
    "no-unused-vars": "off",
    "no-unexpected-multiline": "off"
  }
}
